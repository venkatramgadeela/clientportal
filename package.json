{
  "name": "dct-client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "pm2 startOrReload config/pm2-prod.json",
    "build": "npm run build-webpack && npm run build-node && npm run build-gulp",
    "build-clean": "rm -rf build && mkdir build",
    "build-webpack": "node config/scripts/build.js",
    "build-node": "babel src/server -d build/src/server --ignore '**/dev-entry.js'",
    "build-gulp": "gulp",
    "test": "node config/scripts/test.js --env=jsdom",
    "lint": "eslint .",
    "lint-format": "prettier --config .prettierrc --write \"src/**/*.{js,jsx,json,css}\"",
    "lint-fix": "eslint . --fix",
    "lint-rule-conflict-check": "eslint --print-config .eslintrc | eslint-config-prettier-check",
    "precommit": "lint-staged",
    "dev-start": "node config/scripts/start.js",
    "dev-server": "pm2 start config/pm2-dev.json --no-daemon"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
      "prettier --config .prettierrc --write",
      "git add"
    ]
  },
  "dependencies": {
    "@fortawesome/fontawesome": "^1.1.2",
    "@fortawesome/fontawesome-free-solid": "^5.0.4",
    "@fortawesome/react-fontawesome": "^0.0.17",
    "autoprefixer": "7.1.6",
    "axios": "^0.17.1",
    "babel-core": "6.26.0",
    "babel-eslint": "7.2.3",
    "babel-loader": "7.1.2",
    "babel-preset-react-app": "^3.1.1",
    "babel-runtime": "6.26.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "~1.18.2",
    "bootstrap": "^4.1.1",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "classnames": "^2.2.5",
    "compression": "^1.7.1",
    "connect-mongo": "^2.0.1",
    "cookie-parser": "~1.4.3",
    "css-loader": "0.28.7",
    "dotenv": "4.0.0",
    "dotenv-expand": "4.0.1",
    "eslint": "4.9.0",
    "eslint-config-airbnb": "16.1.0",
    "eslint-config-react-app": "^2.1.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "2.39.1",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.4.0",
    "express": "~4.15.5",
    "express-session": "^1.15.6",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.5",
    "fs-extra": "3.0.1",
    "gulp": "^3.9.1",
    "helmet": "^3.11.0",
    "history": "^4.7.2",
    "html-webpack-plugin": "2.29.0",
    "jest": "20.0.4",
    "jsonwebtoken": "^8.1.1",
    "jwt-decode": "^2.2.0",
    "lusca": "^1.5.2",
    "mongoose": "^5.0.0",
    "mongoose-auto-increment": "^5.0.1",
    "morgan": "~1.9.0",
    "object-assign": "4.1.1",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "pm2": "^3.0.0",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.8",
    "promise": "8.0.1",
    "prop-types": "^15.6.0",
    "raf": "3.4.0",
    "react": "^16.2.0",
    "react-bootstrap-table-next": "^0.1.14",
    "react-dev-utils": "^5.0.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.9",
    "reactstrap": "^5.0.0-alpha.4",
    "redux": "^3.7.2",
    "redux-devtools-extension": "^2.13.2",
    "redux-form": "^7.2.1",
    "redux-promise-middleware": "^5.0.0",
    "redux-thunk": "^2.2.0",
    "style-loader": "0.19.0",
    "sw-precache-webpack-plugin": "0.11.4",
    "url-loader": "0.6.2",
    "validator": "^9.2.0",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.9.4",
    "webpack-manifest-plugin": "1.3.2",
    "whatwg-fetch": "2.0.3",
    "winston": "^3.0.0",
    "winston-mongodb": "^4.0.0-rc1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-prettier": "^2.6.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.2.0",
    "prettier": "^1.10.2"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/client/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/client/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/client/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "mjs",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  }
}
